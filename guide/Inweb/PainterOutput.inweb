Language "PainterOutput" {
	colouring
		number 1 of 2 => !plain
		number 2 of 2 => {
			characters {
				"!" => !comment
				"c" => !character
				"d" => !definition
				"e" => !element
				"f" => !function
				"i" => !identifier
				"n" => !constant
				"p" => !plain
				"r" => !reserved
				"s" => !string
				"x" => !extract
			}
		}
	end
}
